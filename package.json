{
  "name": "veggie-t-rex",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/drandx/dropin-spots-service.git"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp dist",
    "build": "npm run clean & tsc -p tsconfig.release.json & cp -R serverless.yml ./dist/src/ & cp -R package.json ./dist/src/ & cd dist/src/ & npm install --production",
    "dev": "tsc --watch & nodemon dist",
    "devwin": "concurrently --kill-others \"tsc --watch\" \"nodemon dist\"",
    "lint": "eslint src --ext ts",
    "tsc": "tsc",
    "start": "node ./dist/index.js",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "=2.289.0",
    "axios": "^0.19.0",
    "body-parser": "=1.18.2",
    "cors": "=2.8.4",
    "mysql": "=2.14.1",
    "promise-mysql": "=3.0.2",
    "reflect-metadata": "=0.1.12",
    "tslib": "~1.6.0",
    "uuid": "=3.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "=1.16.8",
    "@types/cors": "=2.8.4",
    "@types/jest": "=19.2.4",
    "babel-jest": "=19.0.0",
    "concurrently": "^4.1.0",
    "jest": "=20.0.4",
    "ts-jest": "=19.0.6",
    "typescript": "=2.9.2",
    "typescript-eslint-parser": "=3.0.0",
    "rimraf": "~2.6.1"
  },
  "jest": {
    "globals": {
      "__TEST__": "test"
    },
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "<rootDir>/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/**/*.d.ts"
    ]
  }
}
