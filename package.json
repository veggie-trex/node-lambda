{
  "name": "veggie-t-rex",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/drandx/dropin-spots-service.git"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp dist",
    "build": "npm run clean; tsc -p tsconfig.release.json; cp -R serverless.yml ./dist/src/; cp -R package.json ./dist/src/; cd dist/src/; npm install --production",
    "dev": "tsc --watch & nodemon dist",
    "devwin": "concurrently --kill-others \"tsc --watch\" \"nodemon dist\"",
    "lint": "eslint src --ext ts",
    "tsc": "tsc",
    "start": "node ./dist/index.js",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/maps": "^0.5.5",
    "aws-sdk": "=2.289.0",
    "body-parser": "=1.18.2",
    "cors": "=2.8.4",
    "express": "=4.15.3",
    "geolib": "=3.0.1",
    "inversify": "=4.13.0",
    "mongodb": "=3.1.1",
    "morgan": "=1.9.0",
    "nconf": "=0.10.0",
    "nodemon": "=1.11.0",
    "reflect-metadata": "=0.1.12",
    "uuid": "=3.3.2",
    "winston": "=2.4.1"
  },
  "devDependencies": {
    "@types/body-parser": "=1.16.8",
    "@types/cors": "=2.8.4",
    "@types/express": "=4.11.1",
    "@types/jest": "=19.2.4",
    "@types/mongodb": "=3.1.14",
    "@types/nconf": "=0.0.34",
    "@types/node": "=7.0.55",
    "@types/sequelize": "=4.27.8",
    "@types/winston": "=2.3.8",
    "babel-jest": "=19.0.0",
    "concurrently": "^4.1.0",
    "jest": "=20.0.4",
    "ts-jest": "=19.0.6",
    "typescript": "=2.9.2",
    "typescript-eslint-parser": "=3.0.0",
    "rimraf": "~2.6.1"
  },
  "jest": {
    "globals": {
      "__TEST__": "test"
    },
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "<rootDir>/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/**/*.d.ts"
    ]
  }
}
